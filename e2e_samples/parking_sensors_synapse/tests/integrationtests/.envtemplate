###
# REQUIRED. Service principal account details used to connect to Azure, if its STG environment, fill in corresponding ENV's Service Principal Info
AZ_SERVICE_PRINCIPAL_ID=
AZ_SERVICE_PRINCIPAL_SECRET=
AZ_SERVICE_PRINCIPAL_TENANT_ID=

###
# REQUIRED. For Azure related resource like Azure Synapse, fill in corresponding ENV(ex: stg)'s info:
AZ_SUBSCRIPTION_ID=
AZ_RESOURCE_GROUP_NAME=
AZ_SYNAPSE_NAME=

###
# REQUIRED. Azure Synapse output(s) connection information, fill in corresponding ENV(ex: stg)'s info::
AZ_SYNAPSE_DEDICATED_SQLPOOL_NAME={synapseDedicatedSqlPoolName}.sql.azuresynapse.net
AZ_SYNAPSE_SQLPOOL_ADMIN_USERNAME=
AZ_SYNAPSE_SQLPOOL_ADMIN_PASSWORD=
AZ_SYNAPSE_DEDICATED_SQLPOOL_DATABASE_NAME=


###
# OPTIONAL. Environment variables for other non-required configuration.
# Uncomment to set values.

# Poll interval in seconds for checking for pipeline status. *Default*: 5.
# AZURE_SYNAPSE_POLL_INTERVAL=