{
  "cluster_name": "ddo_cluster",
  "autoscale": {
    "min_workers": 1,
    "max_workers": 2
  },
  "spark_version": "14.3.x-scala2.12",
  "autotermination_minutes": 30,
  "node_type_id": "Standard_D4pls_v6",
  "data_security_mode": "SHARED",
  "runtime_engine": "PHOTON",
  "spark_env_vars": {
    "PYSPARK_PYTHON": "/databricks/python3/bin/python3",
    "MOUNT_DATA_PATH": "/mnt/datalake",
    "MOUNT_DATA_CONTAINER": "datalake",
    "DATABASE": "datalake"
  }
}
